
cvmfs_test_name="ATLAS benchmark"
cvmfs_test_autofs_on_startup=false
cvmfs_benchmark="yes"

FQRN=atlas.cern.ch


cvmfs_run_benchmark() {
  export ATLAS_LOCAL_ROOT_BASE=/cvmfs/atlas.cern.ch/repo/ATLASLocalRootBase
  source ${ATLAS_LOCAL_ROOT_BASE}/user/atlasLocalSetup.sh
  /cvmfs/atlas.cern.ch/repo/ATLASLocalRootBase/x86_64/AtlasSetup/V00-04-09/AtlasSetup/scripts/asetup.sh 19.0.0
}

cvmfs_run_test() {
  logfile=$1

  mkdir -p /cvmfs/sft.cern.ch
  mount -t cvmfs sft.cern.ch /cvmfs/sft.cern.ch > /dev/null 2>&1

  run_benchmark

  umount sft.cern.ch > /dev/null 2>&1

  return 0
}
