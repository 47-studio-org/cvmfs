
cvmfs_test_name="ALICE benchmark"


cvmfs_run_test() {
  logfile=$1
  FQRN=alice.cern.ch
  COMMAND='. /cvmfs/alice.cern.ch/etc/login.sh
          alienv enter AliRoot/v5-04-Rev-09-1'

  setup_benchmark_environment $(pwd)

  # use or not warm cache
  if [ "$CVMFS_OPT_WARM_CACHE" == "yes" ]; then
    echo "Starting initial execution"
    create_warm_cache
  else
    echo "Using cold cache"
  fi

  # start the iterations
  run_benchmark

  # collect the results
  collect_benchmark_results

  return 0
}
