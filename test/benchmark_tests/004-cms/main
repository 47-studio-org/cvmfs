
cvmfs_test_name="CMS benchmark"
cvmfs_test_autofs_on_startup=false
cvmfs_benchmark="yes"

FQRN=cms.cern.ch


cvmfs_run_benchmark() {
  . /cvmfs/cms.cern.ch/cmsset_default.sh
  /cvmfs/cms.cern.ch/common/scramv1 project CMSSW CMSSW_7_0_0 # receives a warning
  cd CMSSW_7_0_0
  eval `scramv1 runtime -sh`
  #/cvmfs/cms.cern.ch/slc6_amd64_gcc481/cms/cmssw/CMSSW_7_0_0/bin/slc6_amd64_gcc481/cmsShow # launches a GUI
}

cvmfs_run_test() {
  logfile=$1

  run_benchmark

  return 0
}
