
cvmfs_test_name="CMS benchmark"


cvmfs_run_test() {
  logfile=$1
  FQRN=cms.cern.ch
  COMMAND='. /cvmfs/cms.cern.ch/cmsset_default.sh
          scramv1 project CMSSW CMSSW_7_0_0'

  setup_benchmark_environment $(pwd)

  # use or not warm cache
  if [ "$CVMFS_OPT_WARM_CACHE" == "yes" ]; then
    echo "Starting initial execution"
    create_warm_cache
  else
    echo "Using cold cache"
  fi

  # start the iterations
  run_benchmark

  # collect the results
  collect_benchmark_results

  return 0
}
