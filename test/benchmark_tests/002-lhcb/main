
cvmfs_test_name="LHCB benchmark"
cvmfs_test_autofs_on_startup=false
cvmfs_benchmark="yes"

FQRN=lhcb.cern.ch


cvmfs_run_benchmark() {
  source /cvmfs/lhcb.cern.ch/etc/login.sh
  /cvmfs/lhcb.cern.ch/lib/lhcb/LBSCRIPTS/LBSCRIPTS_v8r3/InstallArea/scripts/SetupProject.sh Panoramix
}

cvmfs_run_test() {
  logfile=$1

  run_benchmark

  return 0
}
